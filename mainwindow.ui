             <item row="8" column="1">
              <widget class="QLineEdit" name="editTscSyncTimeout">
               <property name="minimumSize">
                <size>
                 <width>150</width>
                 <height>0</height>
                </size>
               </property>
               <property name="maximumSize">
                <size>
